ex:PersonShape
  rdf:type sh:NodeShape ;
  sh:property ex:PersonShape-pob ;
  sh:targetQuery " exists (focus.`neo4j://graph.schema#hasPob`)  " ;
.

ex:PersonShape-pob
  sh:path neo:pob ;
  sh:minCount 1 ;
  #sh:maxCount 1 ;
.